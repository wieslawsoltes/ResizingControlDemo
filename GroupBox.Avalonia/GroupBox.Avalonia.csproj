<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netstandard2.0;net6.0</TargetFrameworks>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <LangVersion>latest</LangVersion>
    <IsPackable>True</IsPackable>
    <RootNamespace>GroupBox.Avalonia</RootNamespace>
  </PropertyGroup>

  <PropertyGroup>
    <VersionPrefix>11.0.0</VersionPrefix>
    <VersionSuffix></VersionSuffix>
    <Authors>Wiesław Šoltés</Authors>
    <Company>Wiesław Šoltés</Company>
    <Copyright>Copyright © Wiesław Šoltés 2025</Copyright>
    <PackageProjectUrl>https://github.com/wieslawsoltes/GroupBox.Avalonia</PackageProjectUrl>
    <Description>An GroupBox control for Avalonia.</Description>
    <PackageId>GroupBox.Avalonia</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageTags>groupbox;xaml;control;avalonia;avaloniaui</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Avalonia" Version="11.0.0"/>
  </ItemGroup>

</Project>
